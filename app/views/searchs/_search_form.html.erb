<% if user_signed_in? %>
  <%= form_tag(search_path, method: :get) do %>
    <%= text_field_tag 'content' %>
    <%= select_tag 'model', options_for_serect({"User" => "user", "Book" => "book"}) %>
    <%= select_tag 'method', options_for_serect({"完全一致" => "perfect", "部分一致" => "partial"}) %>
    <%= submit_tag '検索' %>
  <% end %>
<% end 